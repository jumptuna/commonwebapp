</head>
<body>
<div class = "background">
<!-- <div class = "commonlogo">
  <%= image_tag "logo", {:class => "logos"}%>
  

</div>
<div class = "commonlogo2">
<%= image_tag "logo", {:class => "logos2"}%>
</div> -->
<div class = "wrapper">
<div class = "col-sm-12 header">
  <div class = "col-sm-2 logo1">
    <%= link_to image_tag('top_logo.png'), home_path, {:class => "logo"} %>
  </div>
  <div class = "col-sm-2 button">
    <%= link_to "Sign Out", signout_path %>
  </div>
</div>

<div class = "col-sm-10 col1">
<div class = "col-sm-12 broad">
  <div class = "outline1">
  
  <div class ="col-sm-12 prof">
    <div class = "col-sm-4 profile">
      <%= image_tag @cuser.avatar.url.to_s, {:class => "avatar"} %>

    </div>
    <div class = "col-sm-8 basicinfo">
      <p>
          <%= @cuser.name %>
      </p>
      <p>
          1162 Pacific Street
      </p>
    </div>
    </div>
  </div>
</div>

<div class = "col-sm-12 bottomwrap">
  <div class = "col-sm-3 event">
    <div class = "col-sm-12 eventheader">
      <%=link_to "Events", events_path %>
    </div>
    <div class = "col-sm-12 eventcontent">
    </div>
  </div>
  <div class = "col-sm-8 bio">
  <%= @cuser.info %>
  </div>
</div>
</div>
<div class = "col-sm-2 list">

<div class = "col-sm-12 listhead">
<div class = "outline2">

Members
</div>
</div>

<div class = "col-sm-12 innerlist">

<%= render "users/list/list2" %>

</div>
</div>
</div>
<div class = "col-sm-12 footer">
  
  <div class = "col-sm-12 outline3">
      <p></p>
      <div class = "col-sm-4">
      </div>
      <div class = "col-sm-4 weare">
      We Are Common
      </div>
      <div class = "col-sm-12 buffline">
      </div>
      <div class = "col-sm-5">
      </div>
      <div class = "col-sm-2">
      <%= link_to "Edit", edit_user_registration_path %>
      </div>
  </div>
</div>
</div>


</body>
</html>