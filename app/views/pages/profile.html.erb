    width: 80em;
    margin-left: 5em;
    margin-top: 14em;

    }
    .logos2 {
    height: 20em;
    width: 80em;
    margin-left: 96em;
    margin-top: 14em;

    }
    .commonlogo2 {
    position:absolute;
    }
    .outline1 {
    width: 42em;
    height: 21em;
    border-width: .3em;
    border-style: solid;
    border-color: rgb(61, 72, 139);
    margin: 0 auto;
    margin-top: 1.5em;

    }
    .outline2 {
    height: 2.2em;
    width: 8em;
    border-width: .2em;
    border-style: solid;
    border-color: rgb(61, 72, 139);
    margin-top: .4em;
    margin-left: .4em;
    padding-top: .2em;
    }
    .outline3 {
    margin: 0 auto;
    margin-top: .675em;
    height: 7em;


    border-width: .2em;
    border-style: solid;
    border-color: rgb(61, 72, 139);
    padding: 0;
    border-right-width: 0;
    border-left-width: 0;



    }


